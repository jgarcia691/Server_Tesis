                { "key": "id_sede", "value": "1", "type": "text" },
                { "key": "estado", "value": "activo", "type": "text" },
                { "key": "archivo_pdf", "type": "file", "src": "/ruta/a/archivo.pdf" }
              ]
            }
          }
        },
        { "name": "Descargar PDF de tesis", "request": { "method": "GET", "url": "{{baseUrl}}/api/tesis/10/download" } },
        { "name": "Eliminar tesis", "request": { "method": "DELETE", "url": "{{baseUrl}}/api/tesis/10" } },
        {
          "name": "Actualizar tesis (con PDF opcional)",
          "request": {
            "method": "PUT",
            "url": "{{baseUrl}}/api/tesis/10",
            "body": {
              "mode": "formdata",
              "formdata": [
                { "key": "nombre", "value": "Nuevo título", "type": "text" },
                { "key": "fecha", "value": "2024-02-01", "type": "text" },
                { "key": "estado", "value": "revisado", "type": "text" },
                { "key": "archivo_pdf", "type": "file", "src": "/ruta/a/nuevo.pdf" }
              ]
            }
          }
        }
