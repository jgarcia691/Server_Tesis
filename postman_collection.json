                { "key": "id_sede", "value": "1", "type": "text" },
                { "key": "estado", "value": "activo", "type": "text" },
                {
                  "key": "archivo_pdf",
                  "type": "file",
                  "src": "/ruta/a/archivo.pdf"
                }
              ]
            }
          }
        },
