        {
          "name": "Obtener sede por id",
          "request": { "method": "GET", "url": "{{baseUrl}}/api/sede/search/1" }
        },
        {
          "name": "Crear sede",
          "request": {
            "method": "POST",
            "header": [ { "key": "Content-Type", "value": "application/json" } ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"id\": 1,\n  \"nombre\": \"Sede Central\",\n  \"Direccion\": \"Av. Principal\",\n  \"telefono\": \"000-000\"\n}"
            },
            "url": "{{baseUrl}}/api/sede/"
          }
        },
        {
          "name": "Eliminar sede",
          "request": { "method": "DELETE", "url": "{{baseUrl}}/api/sede/1" }
        }
