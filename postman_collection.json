        {
          "name": "Subir tesis con PDF",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/tesis",
            "body": {
              "mode": "formdata",
              "formdata": [
                { "key": "id_tesis", "value": "10", "type": "text" },
                { "key": "nombre", "value": "Tesis de ejemplo", "type": "text" },
                { "key": "id_estudiante", "value": "300", "type": "text" },
                { "key": "id_tutor", "value": "123", "type": "text" },
                { "key": "id_encargado", "value": "200", "type": "text" },
