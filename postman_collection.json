      "item": [
        { "name": "Listar tesis", "request": { "method": "GET", "url": "{{baseUrl}}/api/tesis" } },
        { "name": "Buscar tesis por nombre", "request": { "method": "GET", "url": "{{baseUrl}}/api/tesis/cadena/proyecto" } },
